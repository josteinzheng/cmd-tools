[user]
	name = zhengzhijie
	email = zhengzhijie@we.com
[color]
	ui = auto
[core]
	editor = vim
	whitespace = cr-at-eol
[merge]
	tool = vimdiff
[alias]
	unstage = reset HEAD --
	discard = checkout --
	last = log -2
	st = status
	ll = log --oneline -10
	di = diff
	ci = commit
	br = branch
	co = "!f(){ git checkout \"$@\" && git submodule update --init --recursive; }; f"
	sdi = diff --staged
	delete = "!f(){ git branch -D \"$@\" && git push --delete origin \"$@\"; }; f"
[review "URL"]
	autoupload = true
[push]
	default = simple
[credential]
	helper = cache --timeout=36000
[url "git@gitlab.we.com:we-tech-fe/"]
	insteadOf = http://gitlab.we.com/we-tech-fe/
